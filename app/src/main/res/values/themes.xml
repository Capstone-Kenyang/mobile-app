<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.Kenyang" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <item name="colorPrimary">@color/stone_50</item>
    </style>

    <style name="AppLogo">
        <item name="android:textSize">54sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/blue</item>
    </style>

    <style name="Jargon">
        <item name="android:textSize">18sp</item>
    </style>

    <style name="EditText">
        <item name="android:layout_marginStart">24dp</item>
        <item name="android:layout_marginEnd">24dp</item>
        <item name="android:layout_height">67.5dp</item>
        <item name="android:paddingStart">24dp</item>
        <item name="android:paddingEnd">24dp</item>
        <item name="android:background">@drawable/rounded_corner_rectangle</item>
    </style>

    <style name="Button">
        <item name="android:layout_height">67.5dp</item>
        <item name="android:layout_marginTop">98.5dp</item>
        <item name="android:layout_marginStart">24dp</item>
        <item name="android:layout_marginEnd">24dp</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:backgroundTint">@color/orange</item>
    </style>

    <style name="Poster">
        <item name="android:layout_height">140dp</item>
        <item name="android:layout_marginStart">24dp</item>
        <item name="android:layout_marginEnd">24dp</item>
        <item name="android:layout_marginTop">18dp</item>
    </style>

    <style name="RoundCategory">
        <item name="android:layout_height">65dp</item>
        <item name="android:layout_width">65dp</item>
        <item name="contentDescription">@string/kategori</item>
    </style>

    <style name="CategoryLayout">
        <item name="android:layout_height">90dp</item>
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:layout_marginStart">24dp</item>
    </style>

    <style name="RecommendationImage">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">92dp</item>
        <item name="android:scaleType">fitXY</item>
    </style>

    <style name="RecMenu">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:layout_marginStart">8dp</item>
    </style>

    <style name="Stock">
        <item name="android:textSize">9sp</item>
        <item name="android:textColor">@color/blue</item>
        <item name="android:layout_marginTop">0.25dp</item>
        <item name="android:layout_marginEnd">8dp</item>
    </style>

    <style name="Restaurant">
        <item name="android:textSize">8sp</item>
        <item name="android:textColor">@color/black_2</item>
    </style>

    <style name="RatingColumn">
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/black_4</item>
    </style>

    <style name="Price">
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_marginEnd">6dp</item>
    </style>

    <style name="Title">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="AppBar">
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="NavButton">
        <item name="android:background">@color/blue</item>
    </style>

    <style name="NavText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">1dp</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/zinc_50</item>
        <item name="android:includeFontPadding">false</item>
    </style>

    <style name="ImageOrder">
        <item name="android:layout_marginStart">20dp</item>
        <item name="android:layout_marginTop">16.7dp</item>
        <item name="android:layout_marginBottom">16.7dp</item>
    </style>

    <style name="TvOrderId">
        <item name="android:textSize">8sp</item>
        <item name="android:textColor">@color/black_4</item>
    </style>

    <style name="TvOrderMenu">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="TvOrderRestaurant">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/black_2</item>
        <item name="android:includeFontPadding">false</item>
    </style>

    <style name="TvOrderStatus">
        <item name="android:textSize">8sp</item>
        <item name="android:textColor">@color/black_2</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginTop">11.5dp</item>
        <item name="android:background">@drawable/accent_rounded_grey</item>
    </style>

    <style name="DetailOrderId">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/black_3</item>
        <item name="android:layout_marginTop">27dp</item>
    </style>

    <style name="DetailOrderMenu">
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginTop">12dp</item>
    </style>

    <style name="DetailOrderRestaurant">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/black_2</item>
    </style>

    <style name="DetailOrderAddress">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black_4</item>
        <item name="android:layout_marginTop">15dp</item>
        <item name="android:layout_marginEnd">44dp</item>
        <item name="android:layout_marginStart">44dp</item>
    </style>

    <style name="DetailOrderTvLocation">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/blue_800</item>
        <item name="android:layout_marginTop">30dp</item>
    </style>

    <style name="DetailOrderTvTotal">
        <item name="android:textSize">26sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_marginTop">47dp</item>
    </style>

    <style name="DetailOrderPrice">
        <item name="android:textSize">26sp</item>
        <item name="android:textColor">@color/blue</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="Theme.Kenyang" parent="Base.Theme.Kenyang" />

</resources>